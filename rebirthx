local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()


local Window = OrionLib:MakeWindow({Name = "VernyHub", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


local Main = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MainSection = Main:AddSection({
	Name = "AutoFarm"
})

OrionLib:MakeNotification({
	Name = "Script Loaded!",
	Content = "The Script has succesfully loaded!",
	Image = "rbxassetid://4483345998",
	Time = 5
})

--autoclick

Main:AddToggle({
	Name = "AutoClicker",
	Default = false,
	Callback = function(ATValue)
		_G.autotap = ATValue;

        task.spawn(function()
            while _G.autotap do
game:GetService("ReplicatedStorage").Events.Click3:FireServer()
wait(0.01)
            end
        end)
	end    
})


--autorebirth

Main:AddToggle({
	Name = "AutoRebirth",
	Default = false,
	Callback = function(ATRValue)
		_G.togrev = ATRValue
	end    
})


local adamishotfr = {}
for i=1,64 do
      table.insert(adamishotfr,i)
end



Main:AddDropdown({
	Name = "Select The Option Rebirths",
	Default = " ",
	Options = {unpack(adamishotfr)},
	Callback = function(DRValue)
		_G.drebirth = DRValue
        print(_G.drebirth)
	end    
})


task.spawn(function()
    while task.wait() do
        if _G.togrev then
            if _G.drebirth ~= nil then
                local args = {
                    [1] = _G.drebirth
                }
                game:GetService("ReplicatedStorage").Events.Rebirth:FireServer(unpack(args))
            end
        end
    end
end)


--auto egg

local Main = Window:MakeTab({
	Name = "AutoEgg",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local MainSection = Main:AddSection({
	Name = "Auto Open EGGS"
})



local adamissmartfr3x = {}

for i, v in pairs(game:GetService("Workspace").Scripts.Eggs:GetChildren()) do
	if v:IsA("Model") then
		if not table.find(adamissmartfr3x, v.Name) then
			table.insert(adamissmartfr3x, v.Name)
		end
	end
end



Main:AddToggle({
	Name = "AutoOpen3X",
	Default = false,
	Callback = function(TEValue)
		_G.toegg = TEValue
	end    
})


Main:AddDropdown({
	Name = "Select The Egg You Want To Open",
	Default = " ",
	Options = adamissmartfr3x,
	Callback = function(DEValue)
		_G.open = DEValue
		print(_G.open)
	end    
})


task.spawn(function()
	while task.wait() do
		if _G.toegg then
			if _G.open ~= nil then
				local args = {
					[1] = _G.open,
					[2] = "Triple"
				}
				game:GetService("ReplicatedStorage").Functions.Unbox:InvokeServer(unpack(args))
				wait(0.1)
			end
		end
	end
end)


