--walkspeed


Main:AddSlider({
	Name = "Change Walkspeed",
	Min = 50,
	Max = 400,
	Default = 50,
	Color = Color3.fromRGB(255,255,255),
	Increment = 1,
	ValueName = "WalkSpeed",
	Callback = function(SSValue)
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = SSValue
	end    
})


--teleport world

function teleportTO(placeCFrame)
	local plyr = game.Players.LocalPlayer;
	if plyr.Character then
		plyr.Character.HumanoidRootPart.CFrame = placeCFrame;
	end
end
function teleportWorld(world)
	if game:GetService("Workspace").Scripts.Portals:FindFirstChild(world) then
		teleportTO(game:GetService("Workspace").Scripts.Portals[world].Touch.CFrame)
	end
end



local selectedWorld;

Main:AddDropdown({
	Name = "SelectWorld",
	Default = " ",
	Options = {"Forest", "Beach", "Atlantis", "Desert", "Winter", "Volcano", "Cube", "Moon", "Cyber", "Magic", "Heaven", "Nuclear", "Void", "Spooky", "Cave", "Steampunk", "Hell", "Space", "Hacker", "Mars", "Alien", "Space", "Galaxy Forest", "Neon", "Shadow", "Destruction", "Sun", "Saturn", "Black Hole",},
	Callback = function(SWValue)
		selectedWorld2 = SWValue
		print(selectedWorld2)
	end    
})

Main:AddButton({
	Name = "teleport to selected world",
	Callback = function()
		if selectedWorld2 then
			teleportWorld(selectedWorld2)
		end
  	end    
})



