end)
--misc

Main:AddToggle({
	Name = "AutoCraftAll",
	Default = false,
	Callback = function(CAValue)
		_G.autocraft = CAValue;

		task.spawn(function()
			while _G.autocraft do
				local args = {
					[1] = "CraftAll",
					[2] = {}
				}
				
				game:GetService("ReplicatedStorage").Functions.Request:InvokeServer(unpack(args))
				wait(5)
			end
		end)
	end    
})

